{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "google"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "rules": {
    "max-len": [
      "warn",
      {
        "code": 120
      }
    ],
    "linebreak-style": "off",
    "new-cap": "off"
  }
}
